	volume = {43},
	issn = {2168-2267},
	doi = {10.1109/TSMCB.2012.2214381},
	abstract = {In order to minimize steady-state error with respect to uncertainties in robot control, proportional-integral-derivative (PID) control needs a big integral gain, or a neural compensator is added to the classical proportional-derivative (PD) control with a large derivative gain. Both of them deteriorate transient performances of the robot control. In this paper, we extend the popular neural PD control into neural PID control. This novel control is a natural combination of industrial linear PID control and neural compensation. The main contributions of this paper are semiglobal asymptotic stability of the neural PID control and local asymptotic stability of the neural PID control with a velocity observer which are proved with standard weight training algorithms. These conditions give explicit selection methods for the gains of the linear PID control. An experimental study on an upper limb exoskeleton with this neural PID control is addressed.},
	number = {2},
	journal = {IEEE Transactions on Cybernetics},
	author = {Yu, W. and Rosen, J.},
	month = apr,
	year = {2013},
	keywords = {0, asymptotic stability, Exoskeleton, explicit selection methods, industrial linear PID control, local asymptotic stability, manipulators, neural compensator, Neural networks, neural PD control, neural PID control, neurocontrollers, observers, PD control, PID, proportional-derivative control, proportional-integral-derivative control, robot, robot control, robot manipulators, semiglobal asymptotic stability, Stability analysis, standard weight training algorithms, steady-state error, three-term control, upper limb exoskeleton, velocity observer},
	pages = {673--684},
	file = {IEEE Xplore Abstract Record:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/N5FB284I/6316198.html:text/html;IEEE Xplore Full Text PDF:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/4EFMEEVD/Yu and Rosen - 2013 - Neural PID Control of Robot Manipulators With Appl.pdf:application/pdf}
}

@article{lin_intelligent_2012,
