	month = may,
	year = {2012},
	keywords = {Doubly-fed induction generator, field-oriented control, proportional-integral-derivative neural network},
	pages = {768--783},
	file = {Full Text PDF:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/HIK6HRTK/Lin et al. - 2012 - Intelligent control of doubly-fed induction genera.pdf:application/pdf;Snapshot:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/9IB9AZBK/abstract.html:text/html}
}

@article{cong_novel_2005,
	series = {16th {IFAC} {World} {Congress}},
	title = {A {NOVEL} {PID}-{LIKE} {NEURAL} {NETWORK} {CONTROLLER}},
	volume = {38},
	issn = {1474-6670},
	url = {http://www.sciencedirect.com/science/article/pii/S147466701636253X},
	doi = {10.3182/20050703-6-CZ-1902.00241},
	abstract = {In this paper, a novel PID-like neural network controller (PIDNNC) is created. It is composed of a neural network with no more than 3 neural nodes in hidden layer, and there are an activation feedback and (or) an output feedback in hidden layer, respectively. This special structure makes the network be able to be a P, PI, PD, or PID controller as needed. The proposed controller weights can be updated on-line according to errors caused by non-linear and uncertain factors of system, based on some adaptation mechanism. The resilient back-propagation algorithm with sign instead of the gradient is used to derive the rule of updating network weights. The basic ideas, techniques and analysis are presented. Finally, we give the simulation experiment of a double inverted-pendulum system and the comparison of the effects between the proposed control strategy and the conventional one.},
	number = {1},
	urldate = {2017-02-28},
	journal = {IFAC Proceedings Volumes},
	author = {Cong, Shuang and Li, Guodong and Ji, Beichen},
	year = {2005},
	keywords = {adaptive algorithms, multivariable control system, neural control system design, Neural networks, PID controllers},
	pages = {121--126},
	file = {ScienceDirect Full Text PDF:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/B6958VP9/Cong et al. - 2005 - A NOVEL PID-LIKE NEURAL NETWORK CONTROLLER.pdf:application/pdf;ScienceDirect Snapshot:/home/erik/.mozilla/firefox/mwad0hks.default/zotero/storage/FQQEMPKJ/S147466701636253X.html:text/html}
}